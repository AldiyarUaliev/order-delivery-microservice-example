{
  "schemaName": "orders",
  "primaryKeyColumns": ["aggregateId"],
  "dimensionFieldSpecs": [
    {
      "name": "orderId",
      "dataType": "LONG"
    },
    {
      "name": "aggregateId",
      "dataType": "STRING"
    },
    {
      "name": "accountId",
      "dataType": "LONG"
    },
    {
      "name": "eventId",
      "dataType": "LONG"
    },
    {
      "name": "orderStatus",
      "dataType": "STRING"
    },
    {
      "name": "type",
      "dataType": "STRING"
    },
    {
      "name": "orderLocationLon",
      "dataType": "DOUBLE"
    },
    {
      "name": "orderLocationLat",
      "dataType": "DOUBLE"
    },
    {
      "name": "restaurantId",
      "dataType": "LONG"
    },
    {
      "name": "restaurantLon",
      "dataType": "DOUBLE"
    },
    {
      "name": "restaurantLat",
      "dataType": "DOUBLE"
    },
    {
      "name": "restaurantName",
      "dataType": "STRING"
    },
    {
      "name": "restaurantCity",
      "dataType": "STRING"
    },
    {
      "name": "restaurantCountry",
      "dataType": "STRING"
    },
    {
      "dataType": "BYTES",
      "name": "location_st_point",
      "transformFunction": "toSphericalGeography(stPoint(orderLocationLon,orderLocationLat))"
    },
    {
      "dataType": "BYTES",
      "name": "restaurant_st_point",
      "transformFunction": "toSphericalGeography(stPoint(restaurantLon,restaurantLat))"
    }
  ],
  "dateTimeFieldSpecs": [
    {
      "name": "createdAt",
      "dataType": "LONG",
      "format": "1:MILLISECONDS:EPOCH",
      "granularity": "1:MILLISECONDS"
    }
  ]
}
